		annotation SummarizationSetBy = Automatic

	column Avg_Date_By_Module = ```
			
			CALCULATE(
			    MIN('Max\ Avg'[Max\ Avg]),  
			    ALLEXCEPT('Max\ Avg', 'Max\ Avg'[Max\ Avg]
			)
			
			)
			```
		dataType: string
		summarizeBy: none

