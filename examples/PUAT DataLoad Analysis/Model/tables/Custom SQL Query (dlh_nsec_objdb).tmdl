	column Help = "Help"
		dataType: string
		summarizeBy: none

		annotation SummarizationSetBy = Automatic

	column Golden_Date = IF ( [Max Avg flag] = "True", [FISCAL_DATE], BLANK() )
		dataType: string
		summarizeBy: none

		annotation SummarizationSetBy = Automatic

	column Subject = ```
			
			"Data Load below " & 
			FORMAT(SELECTEDVALUE('Diff Percentage'[Diff Percentage]), "0.00%") & 
			" Of Golden Data Range"
			
			```
		dataType: string
		summarizeBy: none

