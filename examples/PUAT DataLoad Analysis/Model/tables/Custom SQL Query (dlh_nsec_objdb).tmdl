	column 'Max Avg flag' =
			
			IF('Custom SQL Query (dlh_nsec_objdb)'[flag] = "True", 'Custom SQL Query (dlh_nsec_objdb)'[FISCAL_DATE], BLANK())
		dataType: string
		summarizeBy: none

		annotation SummarizationSetBy = Automatic

	column 'week start date' =
