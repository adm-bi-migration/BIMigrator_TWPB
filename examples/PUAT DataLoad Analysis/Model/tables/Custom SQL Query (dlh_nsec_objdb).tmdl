			    MIN('Custom SQL Query (dlh_nsec_objdb)'[FISCAL_DATE]),
			    ALLEXCEPT('Custom SQL Query (dlh_nsec_objdb)', 'Custom SQL Query (dlh_nsec_objdb)'[FISCAL_WEEK])
			)
		dataType: string
		summarizeBy: none

